{% extends 'basebo.html.twig' %}

{% block content %}
    <h1>Eventtype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ eventtype.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ eventtype.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ eventtype.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('eventtype_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('eventtype_edit', { 'id': eventtype.id }) }}">Edit</a>
        </li>

    </ul>
{% endblock %}
